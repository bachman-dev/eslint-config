{
  "name": "@bachman-dev/eslint-config",
  "version": "3.0.1",
  "description": "",
  "homepage": "https://github.com/bachman-dev/eslint-config",
  "author": "Collin Bachman <collin.r68zdn9d@bachman.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bachman-dev/eslint-config"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*.{js,d.ts}"
  ],
  "scripts": {
    "build": "tsdown --dts --format esm src/index.ts",
    "build:docs": "tsx ./scripts/docs.ts && prettier --write ./src/rules/README.md ./src/rules/naming-conventions.md",
    "lint": "tsdown --dts --format esm src/index.ts && eslint . && prettier --check .",
    "lint:ci": "eslint --fix . && prettier --write .",
    "lint:fix": "tsdown --dts --format esm src/index.ts && eslint --fix . && prettier --write ."
  },
  "peerDependencies": {
    "typescript-eslint": "^8.45.0"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@types/node": "24.9.2",
    "eslint-config-prettier": "10.1.8",
    "prettier": "3.7.4",
    "tsdown": "0.15.12",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2"
  },
  "dependencies": {
    "eslint": "^9.36.0"
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd"
}
