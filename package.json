{
  "name": "@bachman-dev/eslint-config",
  "version": "3.0.1",
  "description": "",
  "homepage": "https://github.com/bachman-dev/eslint-config",
  "author": "Collin Bachman <collin.r68zdn9d@bachman.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bachman-dev/eslint-config"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "dist/**/*.{mjs,d.mts}"
  ],
  "scripts": {
    "build": "tsdown --dts --format esm src/index.ts",
    "build:docs": "tsx ./scripts/docs.ts && prettier --write ./src/rules/README.md ./src/rules/naming-conventions.md",
    "lint": "tsdown --dts --format esm src/index.ts && eslint . && prettier --check .",
    "lint:ci": "eslint --fix . && prettier --write .",
    "lint:fix": "tsdown --dts --format esm src/index.ts && eslint --fix . && prettier --write ."
  },
  "peerDependencies": {
    "typescript-eslint": "^8.45.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/node": "24.10.1",
    "eslint-config-prettier": "10.1.8",
    "prettier": "3.7.4",
    "tsdown": "0.17.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.1"
  },
  "dependencies": {
    "eslint": "^9.36.0"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
