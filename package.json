{
  "name": "@bachman-dev/eslint-config",
  "version": "3.0.0",
  "description": "",
  "homepage": "https://github.com/bachman-dev/eslint-config",
  "author": "Collin Bachman <collin.r68zdn9d@bachman.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bachman-dev/eslint-config"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*.{js,d.ts}"
  ],
  "scripts": {
    "build": "tsdown --dts --format esm src/index.ts",
    "build:docs": "tsx ./scripts/docs.ts && prettier --write ./src/rules/README.md ./src/rules/naming-conventions.md",
    "lint": "tsdown --dts --format esm src/index.ts && eslint . && prettier --check .",
    "lint:ci": "eslint --fix . && prettier --write .",
    "lint:fix": "tsdown --dts --format esm src/index.ts && eslint --fix . && prettier --write ."
  },
  "peerDependencies": {
    "typescript-eslint": "^8.45.0"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@types/node": "22.18.1",
    "eslint-config-prettier": "10.1.8",
    "prettier": "3.6.2",
    "tsdown": "0.15.6",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0"
  },
  "dependencies": {
    "eslint": "^9.36.0"
  },
  "packageManager": "pnpm@10.18.0+sha512.e804f889f1cecc40d572db084eec3e4881739f8dec69c0ff10d2d1beff9a4e309383ba27b5b750059d7f4c149535b6cd0d2cb1ed3aeb739239a4284a68f40cfa"
}
